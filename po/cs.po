msgid "Xfce Mixer"
msgstr "Směšovač Xfce"

#: ../xfce4-mixer/xfce-mixer.c:172
msgid "Playback"
msgstr "Přehrávání"

#: ../xfce4-mixer/xfce-mixer.c:172
msgid "Capture"
msgstr "Záznam"


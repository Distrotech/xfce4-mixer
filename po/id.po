msgid "Xfce Mixer"
msgstr "Mixer Xfce"

#: ../xfce4-mixer/xfce-mixer.c:162
msgid "Playback"
msgstr "Putar kembali"

#: ../xfce4-mixer/xfce-mixer.c:162
msgid "Capture"
msgstr "Tangkap"

#: ../xfce4-mixer/xfce-mixer.c:162
msgid "Switches"
msgstr "Tukar"

#: ../xfce4-mixer/xfce-mixer.c:162
msgid "Options"
msgstr "Opsi"

#: ../xfce4-mixer/xfce-mixer.c:289
msgid "No Controls Visible"
msgstr "Tak Ada Kontrol Terlihat"

#: ../xfce4-mixer/xfce-mixer.c:293
msgid "No controls are marked as visible. Please open the <span size='large'><b>Select Controls</b></span> dialog to select some."
msgstr "Tak ada kontrol yang ditandai sebagai terlihat. Silakan buka dialog <span size='large'><b>Pilih Kontrol</b></span> untuk memilih."

#: ../xfce4-mixer/xfce-mixer-controls-dialog.c:129
msgid "Select Controls"
msgstr "Pilih Kontrol"

#: ../xfce4-mixer/xfce-mixer-controls-dialog.c:131
msgid "Please select which controls you want to be visible"
msgstr "Silakan pilih kontrol mana yang anda inginkan untuk terlihat"

#: ../xfce4-mixer/xfce-mixer-window.c:95
msgid "_Quit"
msgstr "Keluar (_Q)"

#: ../xfce4-mixer/xfce-mixer-window.c:95
msgid "Exit Xfce Mixer"
msgstr "Keluar Mixer Xfce"

#: ../xfce4-mixer/xfce-mixer-window.c:97
msgid "_Select Controls..."
msgstr "Pilih Kontrol... (_S)"

#: ../xfce4-mixer/xfce-mixer-window.c:97
msgid "Select which controls are displayed"
msgstr "Pilih kontrol mana yang ditampilkan"

#: ../xfce4-mixer/xfce-mixer-window.c:201
msgid "A comfortable audio mixer for your sound card."
msgstr "Mixer audio yang nyaman untuk kartu suara anda."

#: ../xfce4-mixer/xfce-mixer-window.c:216
msgid "Sound card:"
msgstr "Kartu suara:"

#: ../xfce4-mixer/xfce-mixer-window.c:315
msgid "No sound cards could be found. You may have to install additional gstreamer packages for ALSA or OSS support."
msgstr "Tak ada kartu suara yang dapat ditemukan. Anda mungkin harus menginstal paket gstreamer tambahan untuk dukungan ALSA atau OSS."

#: ../xfce4-mixer/xfce-mixer-window.c:445
#, c-format
msgid "Unknown Volume Control %d"
msgstr "Kontrol Volume %d Tak Dikenal"

#: ../xfce4-mixer/xfce4-mixer.desktop.in.in.h:1
msgid "Audio mixer for the Xfce Desktop Environment"
msgstr "Mixer audio untuk Lingkungan Destop Xfce"


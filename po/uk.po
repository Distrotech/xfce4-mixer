#: ../xfce4-mixer/xfce-mixer-window.c:185
msgid "Sound _card:"
msgstr "Звукова _карта:"

#: ../panel-plugin/xfce-plugin-dialog.c:174
msgid "Mixer _track:"
msgstr "Трек _мікшера"

